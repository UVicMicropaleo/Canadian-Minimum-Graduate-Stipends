```{r scatters}
plot(sos$Endowment[which(sos$Program == "MSc")],
     sos$Net.Stipend[which(sos$Program == "MSc")],
     pch=1,
     lwd=2,
     cex=1.25,
     col="dodgerblue",
     ylim=c(0,max(sos$Net.Stipend,na.rm=T)))
points(sos$Endowment[which(sos$Program == "PhD")],
     sos$Net.Stipend[which(sos$Program == "PhD")],
     pch=16,
     cex=.75,
     col="firebrick2")
cor.test(sos$Endowment,
     sos$Net.Stipend,use="complete.obs")


plot(sos$Operating.Grant,
     sos$Net.Stipend,
     pch=16,
     col="dodgerblue",
     ylim=c(0,max(sos$Net.Stipend,na.rm=T)))
points(sos$Operating.Grant[which(sos$Program == "PhD")],
     sos$Net.Stipend[which(sos$Program == "PhD")],
     pch=16,
     col="firebrick2")
cor.test(sos$Operating.Grant,
     sos$Net.Stipend,use="complete.obs")


plot(sos$Total.Expenses,
     sos$Net.Stipend,
     pch=16,
     col="dodgerblue",
     ylim=c(0,max(sos$Net.Stipend,na.rm=T)))
points(sos$Total.Expenses[which(sos$Program == "PhD")],
     sos$Net.Stipend[which(sos$Program == "PhD")],
     pch=16,
     col="firebrick2")
cor.test(sos$Total.Expenses,
     sos$Net.Stipend,use="complete.obs")






